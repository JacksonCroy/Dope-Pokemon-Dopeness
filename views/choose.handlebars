    <form class="form-inline search">
      <div class="form-group">
        <input type="text" class="form-control" id="search-value2" placeholder="Opponent">
      </div>
      <button id="search-btn2" type="submit" class="btn btn-primary mb-2">Choose</button>
    </form>

  </div>
</div>





